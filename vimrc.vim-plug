call plug#begin('~/.vim/bundle')

Plug 'https://github.com/airblade/vim-gitgutter'
Plug 'https://github.com/bronson/vim-trailing-whitespace'  " :FixWhitespace
Plug 'https://github.com/ctrlpvim/ctrlp.vim'  " ctrl + p to search
Plug 'https://github.com/easymotion/vim-easymotion'  " leader + leader + w
Plug 'https://github.com/lilydjwg/colorizer'  " show color for css
Plug 'https://github.com/majutsushi/tagbar'
Plug 'https://github.com/mattn/emmet-vim'
Plug 'https://github.com/michaeljsmith/vim-indent-object'
Plug 'https://github.com/scrooloose/nerdcommenter'  " leader + c + i to toggle comment
Plug 'https://github.com/scrooloose/nerdtree'
Plug 'https://github.com/tell-k/vim-autopep8'  " F8 to format
Plug 'https://github.com/tmhedberg/matchit' " % to match
Plug 'https://github.com/tomasr/molokai'
Plug 'https://github.com/tpope/vim-fugitive'
Plug 'https://github.com/tpope/vim-repeat' " . to repeat
Plug 'https://github.com/tpope/vim-surround'
Plug 'https://github.com/Valloric/YouCompleteMe', {'do': 'python3 ./install.py'}
Plug 'https://github.com/vim-airline/vim-airline'
Plug 'https://github.com/vim-scripts/YankRing.vim'
Plug 'https://github.com/w0rp/ale'

call plug#end()
