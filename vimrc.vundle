set nocompatible
filetype off

set rtp+=~/.vim/bundle/Vundle.vim
call vundle#begin()

Plugin 'https://github.com/VundleVim/Vundle.vim'  " let Vundle manage Vundle, required!

Plugin 'https://github.com/airblade/vim-gitgutter'
Plugin 'https://github.com/bronson/vim-trailing-whitespace'  " :FixWhitespace
Plugin 'https://github.com/ctrlpvim/ctrlp.vim'  " ctrl + p to search
Plugin 'https://github.com/easymotion/vim-easymotion'  " leader + leader + w
Plugin 'https://github.com/lilydjwg/colorizer'  " show color for css
Plugin 'https://github.com/majutsushi/tagbar'
Plugin 'https://github.com/mattn/emmet-vim'
Plugin 'https://github.com/michaeljsmith/vim-indent-object'
Plugin 'https://github.com/scrooloose/nerdcommenter'  " leader + c + i to toggle comment
Plugin 'https://github.com/scrooloose/nerdtree'
Plugin 'https://github.com/tell-k/vim-autopep8'  " F8 to format
Plugin 'https://github.com/tmhedberg/matchit' " % to match
Plugin 'https://github.com/tomasr/molokai'
Plugin 'https://github.com/tpope/vim-fugitive'
Plugin 'https://github.com/tpope/vim-repeat' " . to repeat
Plugin 'https://github.com/tpope/vim-surround'
Plugin 'https://github.com/vim-airline/vim-airline'
Plugin 'https://github.com/vim-scripts/YankRing.vim'
Plugin 'https://github.com/w0rp/ale'

call vundle#end()
filetype plugin indent on

